{
    "builds": [
      {
        "src": "main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "main.py"
      }
    ],
    "functions": {
        "api/main.py": {
        "memory": 1024,         
        "maxDuration": 60       
        },
        "api/summarizer.py": {
        "memory": 1024,
        "maxDuration": 60
        }
    }
  }